    "@wdio/config": "^5.7.0",
    "@wdio/logger": "^5.7.0",
    "@wdio/sync": "^5.7.1",
    "cucumber": "^5.1.0",
    "fibers": "^3.1.1",
    "glob": "^7.1.2",
    "is-glob": "^4.0.0",
    "mockery": "~2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
